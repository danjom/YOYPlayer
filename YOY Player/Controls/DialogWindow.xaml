<Window x:Class="YOYPlayer.Controls.DialogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YOYPlayer.Controls"
        WindowStyle="None" WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize"
        StateChanged="Window_StateChanged"
        KeyDown="Window_KeyDown" SizeToContent="WidthAndHeight"
        mc:Ignorable="d"
        AllowsTransparency="True" Background="Transparent"
        
        Title="YOY Player" Closed="Window_Closed" Loaded="Window_Loaded">
</Window>